// internal vars
usevar globalvar A;
usevar playervar A;

define globalvar mapID;

rule: "Get map ID"
	Event.Ongoing_EachPlayer

    if (IsButtonHeld(EventPlayer(), Button.Interact) | IsButtonHeld(EventPlayer(), Button.Crouch))

	{
        for (player in AllPlayers())
        {
            SmallMessage(EventPlayer(), AllPlayers()[player]);
            Wait(2);
        }
	}