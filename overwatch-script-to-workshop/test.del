usevar globalvar A;
usevar playervar A;

define playervar isTargetted;

rule: "Resurrect player then send message."
if (IsButtonHeld(EventPlayer(), Button.Interact))
{
    define x;
    define y;
    for (x = 0; x < 10; x+=1;)
        for (y = 0; y < 10; y+=1;)
        {
            SmallMessage(EventPlayer(), <"<0>, <1>", x, y>); 
            Wait(0.5); 
        }
} 